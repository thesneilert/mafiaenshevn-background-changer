{
    "manifest_version": 3,
    "name": "Mafia Background Changer",
    "version": "1.0",
    "description": "Change the background image of mafiaenshevn.com",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://www.mafiaenshevn.com/*"],
        "js": ["contentScript.js"]
      }
    ]
  }
  